{
  "[name]":{
    "region":"[region]",
    "resource":{
      "name":"[name]",
      "network":"projects/[projectId]/global/networks/[name]"
    },
    "forwardingRules":{
      "ESP":{
        "name":"fr-esp",
        "target":"projects/[projectId]/regions/[region]/targetVpnGateways/[name]",
        "IPAddress":"[staticIGoogleP]",
        "IPProtocol":"ESP"
      },
      "UDP":[
        {
          "name":"fr1-udp500",
          "target":"projects/[projectId]/regions/[region]/targetVpnGateways/[name]",
          "IPAddress":"[staticIGoogleP]",
          "IPProtocol":"UDP",
          "portRange":"500"
        },
        {
          "name":"fr1-udp4500",
          "target":"projects/[projectId]/regions/[region]/targetVpnGateways/[name]",
          "IPAddress":"[staticIGoogleP]",
          "IPProtocol":"UDP",
          "portRange":"4500"
        }
      ]
    },
    "vpnTunnels":[
      {
        "name":"[vpnTunnelName]",
        "description":"",
        "targetVpnGateway":"projects/[projectId]/regions/[region]/targetVpnGateways/[name]",
        "ikeVersion":"[ikeVersion]",
        "sharedSecret":"[sharedVPNSecret]",
        "localTrafficSelector":[
          "[googleSubnetworkRange]"
        ],
        "peerIp":"[peerIp]",
        "remoteTrafficSelector":[
          "[remoteSubnetworkRange]"
        ],
        "routes":[
          {
            "name":"vpn-route-1",
            "network":"projects/[projectId]/global/networks/[name]",
            "destRange":"[dest_range]",
            "priority": 1000,
            "nextHopVpnTunnel":"projects/[projectId]/regions/[region]/vpnTunnels/[vpnTunnelName]"
          }
        ]
      }
    ]
  }
}